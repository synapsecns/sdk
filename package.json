{
  "name": "@synapseprotocol/sdk",
  "version": "0.70.0",
  "description": "Synapse Protocol's NodeJS SDK",
  "repository": "github.com/synapsecns/sdk",
  "author": "synapsecns",
  "license": "UNLICENSED",
  "private": false,
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "setup": "yarn && yarn run typegen",
    "test": "mocha",
    "prebuild": "rm -rf ./dist",
    "build": "tsc",
    "prebuild:esbuild": "rm -rf ./dist",
    "build:esbuild": "node esbuild.js",
    "prepublishOnly": "yarn build",
    "publish:patch": "yarn publish --patch",
    "publish:minor": "yarn publish --minor",
    "abigen": "scripts/abigen.sh",
    "typegen": "typechain --target ethers-v5 --out-dir src/contracts/gen src/abis/*.json"
  },
  "dependencies": {
    "@ethersproject/abi": "^5.5.0",
    "@ethersproject/abstract-signer": "^5.5.0",
    "@ethersproject/bignumber": "^5.5.0",
    "@ethersproject/bytes": "^5.5.0",
    "@ethersproject/constants": "^5.5.0",
    "@ethersproject/contracts": "^5.5.0",
    "@ethersproject/providers": "^5.5.0",
    "@ethersproject/units": "^5.5.0",
    "@ethersproject/wallet": "^5.5.0",
    "ethers": "^5.5.1"
  },
  "devDependencies": {
    "@typechain/ethers-v5": "^8.0.1",
    "@types/expect": "^24.3.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.11.4",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "esbuild": "^0.13.13",
    "esbuild-node-externals": "^1.3.0",
    "esbuild-plugin-d.ts": "^1.0.6",
    "esbuild-plugin-tsc": "^0.3.0",
    "mocha": "^9.1.3",
    "mocha-steps": "^1.3.0",
    "ts-mocha": "^8.0.0",
    "ts-node": "^10.4.0",
    "typechain": "^6.0.2",
    "typescript": "^4.4.4"
  }
}
